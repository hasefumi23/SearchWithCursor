{
	"name": "search-with-cursor",
	"displayName": "Search with cursor",
	"description": "Search with cursor in browser.",
	"readme": "README.md",
	"version": "0.1.5",
	"publisher": "hasefumi23",
	"galleryBanner": {
		"color": "#e3f4ff",
		"theme": "light"
	},
	"license": "MIT",
	"keywords": [
		"search",
		"cursor",
		"browser"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/hasefumi23/SearchWithCursor.git"
	},
	"bugs": {
		"url": "https://github.com/hasefumi23/SearchWithCursor/issues"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.searchWithCursor"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.searchWithCursor",
				"title": "Search with cursor"
			}
		],
		"keybindings": [
			{
				"key": "alt+shift+s",
				"command": "extension.searchWithCursor",
				"when": "editorTextFocus"
			}
		]
	},
	"configuration": {
		"title": "Search with cursor Configuration",
		"type": "object",
		"properties": {
			"searchWithCursor.customSearchEngine": {
				"type": "string",
				"default": "https://www.google.com/search?q=%s",
				"description": "Search engine."
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "5.2.6",
		"@types/node": "10.14.1",
		"@types/valid-url": "1.0.2",
		"tslint": "5.14.0",
		"typescript": "3.3.4000",
		"vscode": "1.1.33"
	},
	"dependencies": {
		"@types/opn": "^5.1.0",
		"opn": "^5.4.0"
	}
}
